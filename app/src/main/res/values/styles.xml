<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!--  Customize your theme here.  -->
        <item name="colorPrimary">@color/color_ffffff</item>
        <item name="colorPrimaryDark">@color/color_262626_nochange</item>
        <item name="colorAccent">@color/color_262626_nochange</item>
        <item name="popupMenuStyle">@style/MyPopupMenu</item>
        <item name="android:fontFamily" tools:targetApi="jelly_bean">@font/nunito_sans</item>
    </style>

    <style name="LauncherActivityTheme" parent="@style/Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/color_262626_nochange</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowExitAnimation">@null</item>  <!--//TODO remove-->
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowActionModeOverlay">true</item>
        <item name="android:windowBackground">@drawable/splash_screen_background</item>
    </style>

    <style name="MyPopupMenu" parent="@style/Widget.AppCompat.PopupMenu">
        <item name="android:dropDownHorizontalOffset">-20dp</item>
        <!--<item name="android:textColor">@color/body_small_text</item>-->
        <!--<item name="android:textAppearanceSmall">@style/MyPopupMenuItem</item>-->
        <item name="android:popupBackground">@color/color_ffffff_nochange</item>
    </style>


